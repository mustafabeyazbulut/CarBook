@model CreateAboutDto
@{
    ViewData["Title"] = "CreateAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<div class="card">
    <div class="card-body">

        <h4 class="card-title">Yeni About Girişi</h4>
        <p class="card-subtitle mb-4">

        </p>

        <form method="post">
            <div class="form-group">
                <label for="exampleInputtitle">Özellik</label>
                <input type="text" class="form-control" id="exampleInputtitle" asp-for="title" placeholder="Başlık giriniz">
            </div>
            <div class="form-group">
                <label for="exampleInputdescription">Özellik</label>
                <input type="text" class="form-control" id="exampleInputdescription" asp-for="description" placeholder="Açıklama giriniz">
            </div>
            <div class="form-group">
                <label for="exampleInputimageUrl">Özellik</label>
                <input type="text" class="form-control" id="exampleInputimageUrl" asp-for="imageUrl" placeholder="Video Url giriniz">
            </div>

            <button type="submit" class="btn btn-primary waves-effect waves-light">About Kaydet</button>
        </form>

    </div> <!-- end card-body-->
</div>

